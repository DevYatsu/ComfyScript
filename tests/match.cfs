let x = true

match x {
    true => {
        print("it's true")
    }
    false => {
        print("it's false")
    }
}

import * as fs from "fs"
let file_content = fs.read_file("doesNotExist.txt")

match file_content {
    Ok(content) => {
        print(#"Here is the file content:\n { content }")
    }
    Err(error) => {
        print(#"Error: { error }")
    }
}